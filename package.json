{
  "name": "keyv-dataloader",
  "version": "0.2.0",
  "description": "A DataLoader implementation with caching support using Keyv",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --no-watchman",
    "test:watch": "jest --watch --no-watchman",
    "test:memory": "jest memory --no-watchman",
    "test:redis": "jest redis --no-watchman",
    "test:memcached": "jest memcached --no-watchman",
    "test:docker": "docker-compose up -d && jest --no-watchman && docker-compose down",
    "test:coverage": "jest --coverage --no-watchman",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dataloader",
    "keyv",
    "cache",
    "batching",
    "graphql"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/keyv": "^4.2.0",
    "dataloader": "^2.2.3",
    "jest": "29.7.0",
    "keyv": "^5.3.2",
    "keyv-memcache": "1.3.3",
    "keyv-redis": "1.3.4",
    "ts-jest": "29.2.6",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "dataloader": "^2.2.3",
    "keyv": "^5.0.0"
  }
}
